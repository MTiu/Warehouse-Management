<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users</title>
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  <link rel="stylesheet" href="stylesheets/users.css">
  <link rel="icon" href="/img/Warehome.png" type="image/icon type">
</head>
<body>
  <div class="container">
    <nav>
      <div class="image-container">
        <a href="/dashboard">
          <img src="/img/Warehome.png" alt="" />
        </a>
      </div>
      <div class="image_container">
        <a href="/dashboard">
          <img src="" alt="" />
          <p>Dashboard</p>
        </a>
      </div>
      <div class="image_container">
        <a href="/view">
          <img src="" alt="" />
          <p>View</p>
        </a>
      </div>
      <div class="image_container">
        <a href="/logs">
          <img src="" alt="" />
          <p>Logs</p>
        </a>
      </div>
      <div class="image_container">
        <a href="/users" class="selected">
          <img src="" alt="" />
          <p>Users</p>
        </a>
      </div>
      <div class="image_container">
        <a href="/profile">
          <img src="" alt="" />
          <p>Profile</p>
        </a>
      </div>
    </nav>
    <div id="main">
      <div class="container">
        <div class="bottom">
            <h2>Users <button id="addButton" >Add User</button> </h2>
            <div id="search">
              <input type="value" required="" value="" />
              <label>
                <span style="transition-delay: 0ms">S</span>
                <span style="transition-delay: 50ms">e</span>
                <span style="transition-delay: 100ms">a</span>
                <span style="transition-delay: 150ms">r</span>
                <span style="transition-delay: 200ms">c</span>
                <span style="transition-delay: 250ms">h</span>
              </label>
            </div>  
            <table>
              <thead>
                <th>User ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Username</th>
                <th>Date Created</th>
                <th>View</th>
                <th>Remove</th>
              </thead>
              <tbody></tbody>
            </table>
        </div>
      </div>
  </div>
  <div class="mask"></div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
  <!-- <script src="/scripts/users.js"></script>  -->
</body>
</html>